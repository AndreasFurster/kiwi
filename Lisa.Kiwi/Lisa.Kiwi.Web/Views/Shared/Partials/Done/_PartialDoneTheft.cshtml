@model Report

<fieldset class="summary">
	<legend>@DisplayNames.StolenObject</legend>
	<input name="StolenObject" value="@Model.StolenObject"/>
</fieldset>
<fieldset class="summary">
	<legend>@DisplayNames.DateOfTheft</legend>
	<input name="DateOfTheft" value="@Model.DateOfTheft"/>
</fieldset>

@if (Model.Vehicles != null)
{
	@Html.Partial("~/Views/Shared/Partials/Done/_PartialDoneVehicles.cshtml", Model.Vehicles)
}

@if (Model.Perpetrators != null)
{
	@Html.Partial("~/Views/Shared/Partials/Done/_PartialDonePerpetrators.cshtml", Model.Perpetrators)
}