@{
	ViewBag.Model = new BugReportViewModel();
}
<html lang="nl">
<head>
	<title>@ViewBag.Title</title>
	<meta charset="utf-8">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
	<link href="@Url.Content("~/Content/reporter.css")" type="text/css" rel="stylesheet">
	<script src="//code.jquery.com/jquery-1.11.2.min.js"></script>
    <script>
        if (!window.jQuery) {
            document.write('<script src="/Scripts/jquery-1.11.2.min.js"><\/script>');
        }
    </script>
	<script src="~/Scripts/hyphenator.js" type="text/javascript"></script>
	<script src="~/Scripts/bug-report.js" type="text/javascript"></script>
    <script src="~/Scripts/messageReciever.js" type="text/javascript"></script>
	@RenderSection("Scripts", required: false)
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1">
    <script type="text/javascript">
      function resizeIframe(obj){
         {obj.style.height = 0;};
         {obj.style.height = obj.contentWindow.document.body.scrollHeight + 'px';}
      }
    </script>
</head>

<body>
	@Html.Partial("~/Views/Report/_partialBugReport.cshtml")
	<header id="header">
		<h1>Melden + Leerpark</h1>
	</header>
	<script src="~/Scripts/header-resizer.js" type="text/javascript"></script>
	@RenderBody()
	@RenderSection("WeaponScript", required: false)
    <script src="~/Scripts/dashboardLightbox.js" type="text/javascript"></script>
</body>
</html>