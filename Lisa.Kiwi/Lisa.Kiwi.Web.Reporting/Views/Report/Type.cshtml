@model Lisa.Kiwi.Web.Reporting.Models.OriginalReport

@{
    ViewBag.Title = "Type melding";
}

<div class="center-box">
    <h2>Wat wil je melden?</h2>
    @using (Html.BeginForm("Details", "Report"))
    {
        <div class="form-group">
            @Html.DropDownList("reportType", ViewBag.ReportTypes as SelectList)
        </div>
        <input type="submit" name="typeBtn" value="Volgende" />
    
    }
</div>
